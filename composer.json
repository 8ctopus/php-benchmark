{
    "name": "8ctopus/php-benchmark",
    "type": "project",
    "description": "Benchmark your php code",
    "keywords": ["speed", "benchmark"],
    "license": "MIT",
    "authors": [
        {
            "name": "8ctopus",
            "email": "hello@octopuslabs.io"
        }
    ],
    "require": {
        "8ctopus/apix-log": "^3.0",
        "monolog/monolog": "^3.2"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.8"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [{
        "url": "https://github.com/8ctopus/log",
        "type": "git"
    }],
    "scripts": {
        "fix": "php-cs-fixer fix --verbose",
        "fix-risky": "php-cs-fixer fix --allow-risky=yes --verbose"
    }

}
